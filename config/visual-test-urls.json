{
  "pages": [
    {
      "path": "/weather/",
      "name": "top-page",
      "description": "トップページ",
      "viewport": ["desktop", "tablet", "mobile"],
      "mask": [
        "[class*='ad']",
        "[id*='ad']",
        "[class*='Ad']",
        "[id*='Ad']",
        ".advertisement",
        ".advert",
        ".ads",
        "[data-ad]",
        "[data-ads]"
      ]
    },
    {
      "path": "/weather/week/",
      "name": "week",
      "description": "週間天気",
      "viewport": ["desktop", "tablet"],
      "mask": [".timestamp", ".user-name"]
    },
    {
      "path": "/weather/promo/column/",
      "name": "column",
      "description": "お天気コラム",
      "viewport": ["desktop"]
    }
  ],
  "elements": [
    {
      "path": "/weather/",
      "selector": "header",
      "name": "header",
      "description": "ヘッダー要素"
    },
    {
      "path": "/weather/",
      "selector": "footer",
      "name": "footer",
      "description": "フッター要素"
    }
  ]
}

